<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/ic_voucher_base"
    android:padding="18dp">

    <LinearLayout
        android:id="@+id/parentView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingStart="@dimen/mud_dimens_12dp"
        android:paddingVertical="@dimen/mud_dimens_12dp"
        android:paddingEnd="@dimen/mud_dimens_12dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.myxlultimate.component.token.imageView.ImageView
            android:id="@+id/imageView"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginTop="@dimen/mud_dimens_4dp"
            app:imageSource=""
            app:imageSourceType="BASE64"
            app:layout_constraintEnd_toStartOf="@id/title"
            app:layout_constraintStart_toStartOf="@id/parentView"
            app:layout_constraintTop_toTopOf="@id/parentView" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/title"
                style="@style/TextAppearance.MudComponents.H5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxLines="1"
                android:paddingHorizontal="@dimen/mud_dimens_16dp"
                android:text="Mobile Legend Mobile Legend Mobile Legend  "
                android:textColor="@color/mud_palette_basic_black"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintStart_toEndOf="@id/imageView"
                app:layout_constraintTop_toTopOf="@id/parentView" />

            <TextView
                android:id="@+id/dateExpired"
                style="@style/TextAppearance.MudComponents.Body3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingHorizontal="@dimen/mud_dimens_16dp"
                android:paddingTop="@dimen/mud_dimens_4dp"
                android:text="Aktif Sampai 11 Januari 2020"
                android:textColor="@color/mud_palette_basic_dark_grey"
                app:layout_constraintStart_toEndOf="@id/imageView"
                app:layout_constraintTop_toBottomOf="@+id/title" />

            <TextView
                android:id="@+id/reddemButtonActive"
                style="@style/TextAppearance.MudComponents.H6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingHorizontal="@dimen/mud_dimens_16dp"
                android:paddingTop="@dimen/mud_dimens_12dp"
                android:text="@string/organism_voucher_card_option_reddem_active"
                android:textColor="?attr/colorBackgroundPrimary"
                app:layout_constraintStart_toEndOf="@id/imageView"
                app:layout_constraintTop_toBottomOf="@+id/dateExpired" />

            <TextView
                android:id="@+id/reddemButtonInactive"
                style="@style/TextAppearance.MudComponents.H6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/mud_dimens_16dp"
                android:paddingTop="@dimen/mud_dimens_8dp"
                android:text="@string/organism_voucher_card_option_reddem_expired"
                android:textColor="@color/mud_palette_basic_dark_grey"
                android:visibility="gone"
                app:layout_constraintStart_toEndOf="@id/imageView"
                app:layout_constraintTop_toBottomOf="@+id/dateExpired" />

        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>