<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal"
    android:gravity="center">

    <com.myxlultimate.component.token.imageView.ImageView
        android:id="@+id/imageView"
        android:layout_width="64dp"
        android:layout_gravity="center"
        android:layout_height="64dp"
        app:imageSourceType="BASE64"
        app:imageSource=""/>

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="vertical"
        android:paddingStart="16dp"
        android:gravity="center">

        <TextView
            android:id="@+id/titleView"
            style="@style/TextAppearance.MudComponents.H5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/mud_palette_basic_black"
            tools:text="Top Up Dimana Saja"
            android:paddingBottom="4dp"/>

        <TextView
            android:id="@+id/informationView"
            style="@style/TextAppearance.MudComponents.Body2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/mud_palette_basic_black"
            tools:text="Pulsa dapat digunakan untuk membeli Booster atau paket lainnya"/>

    </LinearLayout>

</LinearLayout>